<!DOCTYPE html>
<html>
 <head>
	<title>Circulation</title>
	<meta http-equiv="content-type" content="text/html; charset=utf-8"/>
	<meta http-equiv="cache-control" content="no-cache"/>
	<script type="text/javascript" src="/js/vendor/jquery-1.8.2.min.js"></script>
	<script type="text/javascript" src="/tinymce/jscripts/tiny_mce/jquery.tinymce.js"></script>
	<script type="text/javascript">
 	$().ready(function() {
	   $('textarea.mceEditor').tinymce({
      		script_url : '/tinymce/jscripts/tiny_mce/tiny_mce.js',
   	  });
	});

	function printMe(url) {
	  var w = window.open(url,"printwin","status=0,toolbar=0,location=0,menubar=0,directory=0,resizable=0,height=600,width=800");
    w.focus();
	}

	function listQuery(obj) {
	  var exact = obj.exact.checked;
	  var q     = obj.q.value;
	  
	  if (exact) {
	     obj.q.value = "\"" + q + "\"";
	  }
	
	  return true;
	}

	function countText(obj,id, max) {
	  var elm = document.getElementById(id);
 	  elm.innerHTML = obj.value.length + '/' + max;
	}

	function imposeMaxLength(obj, maxLen) {
  	   return (obj.value.length <= maxLen);
  	}
	</script>
	<link rel="stylesheet" href="/css/style.css" type="text/css" media="screen" charset="utf-8" />
	<link rel="stylesheet" href="/css/circulation.css" type="text/css" media="screen" charset="utf-8" />
 </head>
 <body>
 [% IF params.embed == 1 || embed == 1 %]
 [% ELSE %]
  <div id="head">
    <div id="site-title">
     <a href="http://lib.ugent.be"><img src="/img/logo-boekentoren.png" width="122" height="100" alt="Logo Boekentoren"/></a>
     <h1>Universiteitsbibliotheek Gent</h1>
    </div>
  </div>
 [% END %]
 [% content %]
 </body>
</html>
